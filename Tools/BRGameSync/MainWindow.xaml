<Window x:Class="BRGameSync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BRGameSync"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox Margin="249,68,10,10"/>
        <Button Content="GetLatest" HorizontalAlignment="Left" Margin="489,24,0,0" VerticalAlignment="Top" Click="OnGetLatestClicked"/>
        <Button Content="Reconcile" HorizontalAlignment="Left" Margin="572,24,0,0" VerticalAlignment="Top" Click="OnReconcileClicked"/>
        <Button Content="Commit" HorizontalAlignment="Left" Margin="671,24,0,0" VerticalAlignment="Top" Click="OnCommitClicked"/>
        <TextBox x:Name="textRemote" HorizontalAlignment="Left" Margin="10,12,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="385" IsEnabled="False"/>
        <TextBox x:Name="textLocal" HorizontalAlignment="Left" Margin="10,35,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="385" IsEnabled="False"/>
        <ListBox Margin="10,68,567,10"/>

    </Grid>
</Window>
