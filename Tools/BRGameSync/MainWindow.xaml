<Window x:Class="BRGameSync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BRGameSync"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox Margin="0,68,10,10"/>
        <Button Content="GetLatest" HorizontalAlignment="Right" Margin="0,0,166,402" VerticalAlignment="Bottom" Click="OnGetLatestClicked"/>
        <Button Content="Reconcile" HorizontalAlignment="Right" Margin="0,0,96,402" VerticalAlignment="Bottom" Click="OnReconcileClicked"/>
        <Button Content="Commit" HorizontalAlignment="Right" Margin="0,0,35,402" VerticalAlignment="Bottom" Click="OnCommitClicked"/>
        <TextBox x:Name="textRemote" HorizontalAlignment="Left" Margin="10,12,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="385" IsEnabled="False"/>
        <TextBox x:Name="textLocal" HorizontalAlignment="Left" Margin="10,35,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="385" IsEnabled="False"/>

    </Grid>
</Window>
