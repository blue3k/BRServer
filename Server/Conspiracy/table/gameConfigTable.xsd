						<xs:attribute name="MaxGem" type="xs:int" use="required"/>
						<xs:attribute name="DefaultMoney" type="xs:int" use="required"/>
						<xs:attribute name="MaxMoney" type="xs:int" use="required"/>
						<xs:attribute name="DefaultFriend" type="xs:int" use="required"/>
						<xs:attribute name="MaxFriend" type="xs:int" use="required"/>
						<xs:attribute name="DefaultStamina" type="xs:int" use="required"/>
