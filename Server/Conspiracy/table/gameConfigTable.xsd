						<xs:attribute name="MorningDiscussion" type="xs:int" use="required"/>
						<xs:attribute name="ResultNoticeTime" type="xs:int" use="required"/>
						<xs:attribute name="PlayerDisplayOffline" type="xs:int" use="required"/>
						<xs:attribute name="MaxCharacterSet" type="xs:int" use="required"/>
						<xs:attribute name="ForceProgressCount" type="xs:int" use="required"/>
						<xs:attribute name="PlayerAutoLogout" type="xs:int" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
