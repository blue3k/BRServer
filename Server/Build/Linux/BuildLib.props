<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <OutDir>$(SolutionDir)Server/lib/$(Platform)$(Configuration)\</OutDir>
    <TargetName>libBR$(RootNamespace)</TargetName>
    <RemoteProjectDir>$(RemoteRootDir)/Lib/$(RootNamespace)</RemoteProjectDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="TargetName">
      <Value>$(TargetName)</Value>
    </BuildMacro>
    <BuildMacro Include="RemoteProjectDir">
      <Value>$(RemoteProjectDir)</Value>
    </BuildMacro>
    <BuildMacro Include="OutDir">
      <Value>$(OutDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>