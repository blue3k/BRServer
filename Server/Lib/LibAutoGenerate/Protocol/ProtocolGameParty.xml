<Protocols xmlns="http://www.TheBraves.com/XMLProtocol" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.TheBraves.com/XMLProtocol ProtocolDef.xsd">
	<ProtocolGroup Name="GameParty" GenParameterRouteContext="true" IsMobile="false" IsServer="true" Desc="Game Party Server Protocol">

    <!-- ============================  Party management  ============================================ -->

    <Command Name="JoinParty" Reliable="true" Crypto="No" Trace="DBG1" Desc="Join Party">
		<Cmd>
      <Params Type="PlayerID" Name="InviterID"/>
      <Params Type="PlayerInformation" Name="InvitedPlayer"/>
    </Cmd>
		<Res>
      <Params Type="PlayerID" Name="PartyLeaderID"/>
      <Params Type="uint8" Name="ChatHistoryData" IsArray="true"/>
		</Res>
	</Command>

    <S2CEvent Name="PlayerJoined" Reliable="true" Crypto="No" Trace="DBG1" Desc="Player Joined">
      <Params Type="PlayerInformation" Name="JoinedPlayer"/>
    </S2CEvent>

    <S2CEvent Name="PartyLeaderChanged" Reliable="true" Crypto="No" Trace="DBG1" Desc="Player Joined">
      <Params Type="PlayerID" Name="NewLeaderID"/>
    </S2CEvent>


    <Command Name="LeaveParty" Reliable="true" Crypto="No" Trace="DBG1" Desc="Kick player">
      <Cmd>
        <Params Type="PlayerID" Name="PlayerID"/>
      </Cmd>
      <Res>
      </Res>
    </Command>

   <S2CEvent Name="PlayerLeft" Reliable="true" Crypto="No" Trace="DBG1" Desc="Player left">
		<Params Type="PlayerID" Name="LeftPlayerID"/>
	</S2CEvent>

	
	<Command Name="KickPlayer" Reliable="true" Crypto="No" Trace="DBG1" Desc="Kick player">
		<Cmd>
			<Params Type="PlayerID" Name="PlayerID"/>
			<Params Type="PlayerID" Name="PlayerToKick"/>
		</Cmd>
		<Res>
		</Res>
	</Command>

	<S2CEvent Name="PlayerKicked" Reliable="true" Crypto="No" Trace="DBG1" Desc="Player kicked">
		<Params Type="PlayerID" Name="KickedPlayerID"/>
	</S2CEvent>

	<C2SEvent Name="ChatMessage" Reliable="true" Crypto="No" Trace="DBG1" Desc="Chatting message">
		<Params Type="PlayerID" Name="PlayerID"/>
		<Params Type="String" Name="ChatMessage"/>
	</C2SEvent>

    <S2CEvent Name="ChatMessage" Reliable="true" Crypto="No" Trace="DBG1" Desc="Chatting message">
      <Params Type="PlayerID" Name="SenderID"/>
      <Params Type="String" Name="SenderName"/>
      <Params Type="String" Name="ChatMessage"/>
    </S2CEvent>

    <C2SEvent Name="QuickChatMessage" Reliable="true" Crypto="No" Trace="DBG1" Desc="Quick Chatting message">
      <Params Type="PlayerID" Name="PlayerID"/>
      <Params Type="uint32" Name="QuickChatID" Desc="My quick chat message ID"/>
    </C2SEvent>

    <S2CEvent Name="QuickChatMessage" Reliable="true" Crypto="No" Trace="DBG1" Desc="Quick Chatting message">
      <Params Type="PlayerID" Name="SenderID"/>
      <Params Type="uint32" Name="QuickChatID" Desc="My quick chat message ID"/>
    </S2CEvent>


    <!-- ============================  Party Game matching management  ============================================ -->

    <Command Name="StartGameMatch" Reliable="true" Crypto="No" Trace="DBG1" Desc="Match">
		<Cmd>
			<Params Type="PlayerID" Name="PlayerID"/>
      <Params Type="uint32" Name="MaxGamePlayers"/>
		</Cmd>
		<Res>
		</Res>
	</Command>

    <S2CEvent Name="QueuedGameMatching" Reliable="true" Crypto="No" Trace="DBG1" Desc="Enqueued at a game matching">
      <Params Type="MatchingQueueTicket" Name="MatchingQueueTicket"/>
    </S2CEvent>

    <Command Name="CancelGameMatch" Reliable="true" Crypto="No" Trace="DBG1" Desc="Match">
      <Cmd>
        <Params Type="PlayerID" Name="PlayerID"/>
      </Cmd>
      <Res>
      </Res>
    </Command>

    <S2CEvent Name="CanceledGameMatching" Reliable="true" Crypto="No" Trace="DBG1" Desc="Canceled at a game matching">
      <Params Type="MatchingQueueTicket" Name="MatchingQueueTicket"/>
    </S2CEvent>

    <S2CEvent Name="MatchingItemDequeued" Reliable="true" Crypto="No" Trace="DBG1" Desc="Enqueued item is dequeued">
      <Params Type="MatchingQueueTicket" Name="MatchingTicket"/>
    </S2CEvent>


    <!-- =============================  ======================================= -->

	</ProtocolGroup>
</Protocols>
